{
  "name": "Contempo",
  "description": "C O N T E M P O",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf build/*",
    "copy": "copyfiles -f ./app/static/**/*.*  ./build",
    "build-prod": "npm run clean; npm run copy; webpack --config webpack.prod.js; mv ./build/assets/index.html ./build/",
    "build-app": "npm run clean; npm run copy; webpack --config webpack.app.js",
    "build-staging": "npm run clean; npm run copy; webpack --config webpack.staging.js; mv ./build/assets/index.html ./build/",
    "build-dev": "npm run clean; npm run copy; webpack --config webpack.dev.js; mv ./build/assets/index.html ./build/",
    "start": "webpack-dev-server --config webpack.development.js"
  },
  "devDependencies": {
    "alt": "^0.16.7",
    "alt-container": "^1.0.2",
    "axios": "^0.9.0",
    "babel": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babelify": "^7.0.0",
    "bootstrap-loader": "^1.0.10",
    "browser-sync": "^2.17.0",
    "browser-sync-webpack-plugin": "^1.1.3",
    "chalk": "^1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fixed-data-table": "^0.6.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "immutability-helper": "^2.0.0",
    "imports-loader": "^0.6.5",
    "less": "^2.7.1",
    "moment": "^2.11.1",
    "node-sass": "4.0.0",
    "normalize.css": "^4.1.1",
    "postcss-loader": "^0.9.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "react-nvd3": "^0.5.7",
    "react-router": "^2.3.0",
    "react-select": "^1.0.0-beta13",
    "react-toolbox": "^1.3.0",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "4.1.0",
    "style-loader": "^0.13.1",
    "toolbox-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "settings": {
      "ecmascript": 6,
      "jsx": true
    },
    "rules": {
      "strict": 0,
      "quotes": 0,
      "no-unused-vars": 0,
      "camelcase": 0,
      "no-underscore-dangle": 0
    }
  },
  "dependencies": {
    "antd": "2.9.1",
    "auth0-js": "^7.0.3",
    "auth0-lock": "^9.2.1",
    "bootstrap-sass": "^3.3.6",
    "classnames": "^2.2.5",
    "copyfiles": "^1.0.0",
    "csv": "^1.1.0",
    "font-awesome": "^4.6.3",
    "font-awesome-webpack": "0.0.4",
    "griddle-react": "^1.3.1",
    "history": "^2.1.0",
    "jquery": "^3.0.0",
    "json-loader": "^0.5.4",
    "moment-timezone": "^0.5.13",
    "node-safe-filesaver": "^0.1.0",
    "numeral": "^1.5.3",
    "path": "^0.12.7",
    "prop-types": "^15.5.8",
    "raven-js": "^3.9.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-pure-render-mixin": "^15.3.1",
    "react-addons-shallow-compare": "^15.3.1",
    "react-addons-update": "^15.3.1",
    "react-big-calendar": "^0.14.0",
    "react-dom": "^15.3.1",
    "react-joyride": "^1.5.1",
    "react-js-pagination": "^2.0.2",
    "react-redux": "^5.0.4",
    "react-tooltip": "^3.1.5",
    "recompose": "^0.23.1",
    "rimraf": "^2.5.2",
    "transform-loader": "^0.2.3",
    "uuid-lib": "0.0.6",
    "xlsx": "^0.8.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0",
      "stage-1"
    ],
    "plugins": [
      [
        "import",
        [
          {
            "libraryName": "antd",
            "style": true
          }
        ]
      ]
    ]
  }
}
