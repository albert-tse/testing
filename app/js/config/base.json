{
    "appName": "Contempo",
    "authStorageToken": "contempo_auth_store",
    "userStorageToken": "contempo_user_store",
    "authLevels": {
        "none": "contempo_perm_none",
        "pending": "contempo_perm_pending",
        "authenticated": "contempo_perm_authenticated"
    },
    "routes": {
        "default": "/",

        "login": "/login",
        "loginState": "/login/:state",
        "loginError": "/login/:state/:code",
        "loginErrorHash": "/login/:state/:code/:hash",
        "terms": "/terms",
        "signup": "/signup",
        "success": "/success",
        "publisherPending": "/publisher/pending",

        "home": "/home",

        "explore": "/explore",
        "all": "/all",
        "recommended": "/recommended",
        "trending": "/trending",
        "relevant": "/relevant",
        
        "saved": "/saved",
        "curated": "/curated",
        "internalCurated": "/curated-internal",

        "list": "/list/:listId",
        "related": "/related/:id",
        "articles": "/articles/:ids",

        "analytics": "/analytics",
        "dashboard": "/analytics/dashboard",
        "accounting": "/analytics/accounting",
        "global": "/analytics/global",
        
        "links": "/links",
        
        "settings": "/settings",
        "support": "/support"
    },
    "navItems": [
        {
            "label": "HOME",
            "pathRegex": "home|^\/$",
            "route": "home",
            "icon": "home"
        },
        {
            "label": "EXPLORE",
            "pathRegex": "all|explore|curated|recommended|trending|relevant|saved|list",
            "route": "explore",
            "icon": "explore"
        },
        {
            "label": "ANALYTICS",
            "pathRegex": "analytics",
            "route": "analytics",
            "icon": "trending_up"
        },
        {
            "label": "LINKS",
            "pathRegex": "links",
            "route": "links",
            "icon": "link"
        },
        {
            "label": "SUPPORT",
            "pathRegex": "support",
            "route": "support",
            "icon": "help"
        }
    ],
    "lists":{
        "types": {
            "saved":{
                "id": 1,
                "name": "Saved List",
                "shortname": "saved"
            },
            "2":{
                "list_type_id" : 2,
                "name" : "User Generated",
                "shortname" : "user"
            },
            "3":{       
                "list_type_id" : 3,
                "name" : "Curated - External",
                "shortname" : "curated"
            },
            "4":{
                "list_type_id" : 4,
                "name" : "Curated - Internal",
                "shortname" : "inhouse"
            }
        },
        "names": {
            "all": "All Topics",
            "curated": "Curated",
            "recommended": "Recommended",
            "trending": "Trending",
            "relevant": "Relevant",
            "saved": "Saved",
            "curated-internal": "Internal Testing"
        }
    },
    "shortlinkHostname": "http://qklnk.co/",
    "curTOSVersion": "1",
    "platforms": {
        "1": {
            "id": 1,
            "url": "twitter.com",
            "name": "Twitter"
        },
        "2": {
            "id": 2,
            "url": "facebook.com",
            "name": "Facebook"
        },
        "3": {
            "id": 3,
            "url": "tumblr.com",
            "name": "Tumblr"
        },
        "4": {
            "id": 4,
            "url": "amazon.com",
            "name": "Amazon"
        },
        "6": {
            "id": 6,
            "url": "pinterest.com",
            "name": "Pinterest"
        },
        "7": {
            "id": 7,
            "url": "plus.google.com",
            "name": "Google +"
        },
        "8": {
            "id": 8,
            "url": "email.com",
            "name": "Email"
        }
    },
    "listsOnHome": [
        {
            "type": "special",
            "name": "curated-external",
            "overrides": {
                "list_name": "Curated Posts",
                "icon": "business_center"
            }
        },{
            "type": "special",
            "name": "recommended",
            "overrides": {
                "list_name": "Recommended Posts",
                "icon": "stars"
            }
        },{
            "type": "special",
            "name": "saved",
            "overrides": {
                "list_name": "Recently Saved Posts",
                "icon": "bookmark"
            }
        }
    ],
    "onboardSteps": [
        {
            "title": "Curated Posts",
            "text": "These posts are selected for high performance",
            "selector": ".onboardStep.curated-posts",
            "position": "right"
        },
        {
            "title": "Share Button",
            "text": "Click this icon to share this story directly or get a shortlink",
            "selector": "div[id^='article-']:first-of-type .onboardStep.share-button"
        },
        {
            "title": "Save Button",
            "text": "Click here if you'd like to save this story for later",
            "selector": "div[id^='article-']:first-of-type .onboardStep.save-button"
        }
    ]
}
